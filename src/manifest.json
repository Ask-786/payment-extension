{
  "manifest_version": 3,
  "name": "Zilmoney - Payroll by Credit Card",
  "version": "1.0",
  "description": "Run Payroll from credit card",
  "permissions": [
    "storage",
    "activeTab",
    "scripting"
  ],
  "action": {
    "default_popup": "index.html"
  },
  "icons": {
    "16": "./assets/icons/icon16.png",
    "32": "./assets/icons/icon32.png",
    "48": "./assets/icons/icon48.png",
    "128": "./assets/icons/icon128.png"
  }
}
